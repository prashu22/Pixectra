        </activity>
        <activity android:name=".PhotobookActivity"/>
        <activity android:name=".PosterActivity">


        </activity>
        <activity android:name=".Instagram.InstaMain">

        </activity>
        <activity android:name=".Instagram.AllMediaFiles"/>
        <activity android:name=".Instagram.Relationship"/>

    </application>

</manifest>